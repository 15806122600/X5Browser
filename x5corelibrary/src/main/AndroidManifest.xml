<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.lockyluo.x5corelibrary">

    <!--权限复制开始-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_SETTINGS" />
    <!--<uses-permission android:name="android.permission.WRITE_SETTINGS" />-->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEM" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <!--复制结束-->
    <application

        android:name="com.lockyluo.x5corelibrary.APPAplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true">
        <activity
            android:name="com.lockyluo.x5corelibrary.FullScreenActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.lockyluo.x5corelibrary.FilechooserActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/app_name"></activity>
        <activity
            android:name="com.lockyluo.x5corelibrary.BrowserActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:hardwareAccelerated="true"
            android:label="@string/app_name"
            android:launchMode="standard"></activity>

        <activity
            android:name="com.lockyluo.x5corelibrary.MainUsageActivity"
            android:configChanges="orientation|screenSize|keyboardHidden">
            <!--<intent-filter>-->
                <!--<action android:name="android.intent.action.MAIN" />-->

                <!--<category android:name="android.intent.category.LAUNCHER" />-->
            <!--</intent-filter>-->
        </activity>

    </application>

</manifest>